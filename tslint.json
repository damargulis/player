{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "./node_modules/react-redux-typescript-scripts/tslint.json",
        "./node_modules/react-redux-typescript-scripts/tslint-react.json"
    ],
    "jsRules": {},
    "rules": {
      "ordered-imports": [true, {
        "module-source-path": "basename"
      }],
      "no-unnecessary-initializer": false,
      "interface-name": false,
      "no-any": true,
      "type-def": [true, "call-signature"],
      "curly": true,
      "no-inferred-empty-object-type": true,
      "use-default-type-parameter": true,
      "no-redundant-jsdoc": true,
      "no-import-side-effect": [true, {"ignore-modules": "(\\.html|\\.css)$"}],
      "prefer-method-signature": true
    },
    "rulesDirectory": []
}
